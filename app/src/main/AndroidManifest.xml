<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jayden.custompackage" >

    <application
        android:name=".Common.ApplicationClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Screen.MainScreen.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Screen.SearchScreen.SearchScreenActivity"
            android:label="@string/title_activity_search_screen" >
        </activity>
        <activity
            android:name=".Screen.SearchScreen.AutoCompleteModule.AutoCompleteSearchActivity"
            android:label="@string/title_activity_auto_complete" >
        </activity>
        <activity
            android:name=".Screen.SearchScreen.EditTextModule.EditTextSearchActivity"
            android:label="@string/title_activity_edit_text_search" >
        </activity>
        <activity
            android:name=".Screen.RangeSeekBarScreen.RangeSeekBarActivity"
            android:label="@string/title_activity_range_seek_bar" >
        </activity>
    </application>

</manifest>
